{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2015",
    "lib": [ "ES2015", "dom", "esnext.asynciterable" ],
    "moduleResolution": "Node"
  },
  "include": [ "src/*.ts" ],
  "exclude": [
    "src/*.js",
    "wwwroot/js/*",
    "npm_packages/node_modules/*",
    "webpack.config.js"
  ]
}

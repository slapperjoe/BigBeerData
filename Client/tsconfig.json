{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "lib": [ "es6", "dom", "esnext.asynciterable" ],
    "moduleResolution": "Node"
  },
  "include": [ "wwwroot/ts/*.ts"],
  "exclude": [
    "wwwroot/js/*",
    "npm_packages/node_modules/*"
  ]
}
